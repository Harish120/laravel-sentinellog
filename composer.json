{
  "name": "harryes/laravel-sentinellog",
  "description": "A vigilant authentication logging and security package for Laravel 8-12.",
  "type": "laravel-package",
  "license": "MIT",
  "authors": [
    {
      "name": "Harryes",
      "email": "pantharish120@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0|^8.1|^8.2|^8.3",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/auth": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/notifications": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "guzzlehttp/guzzle": "^7.0",
    "paragonie/constant_time_encoding": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "Harryes\\SentinelLog\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Harryes\\SentinelLog\\SentinelLogServiceProvider"
      ]
    }
  },
  "scripts": {
    "lint": "pint",
    "lint:test": "pint --test",
    "format": "pint -v"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "laravel/pint": "^1.22"
  }
}
