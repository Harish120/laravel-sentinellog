{
  "name": "harryes/laravel-sentinellog",
  "description": "A vigilant authentication logging and security package for Laravel 8-12.",
  "type": "laravel-package",
  "license": "MIT",
  "version": "v0.0.2",
  "keywords": ["laravel", "sentinel", "log", "authentication", "security", "laravel-package"],
  "homepage": "https://github.com/harryes/laravel-sentinellog",
  "authors": [
    {
      "name": "Harryes",
      "email": "pantharish120@gmail.com"
    }
  ],
  "require": {
    "php": "^7.4|^8.0|^8.1|^8.2|^8.3",
    "illuminate/support": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/auth": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/database": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "illuminate/notifications": "^8.0|^9.0|^10.0|^11.0|^12.0",
    "guzzlehttp/guzzle": "^7.0",
    "paragonie/constant_time_encoding": "^2.6"
  },
  "autoload": {
    "psr-4": {
      "Harryes\\SentinelLog\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Harryes\\SentinelLog\\SentinelLogServiceProvider"
      ]
    }
  },
  "scripts": {
    "lint": "pint",
    "lint:test": "pint --test",
    "format": "pint -v",
    "test": "pest",
    "test:coverage": "pest --coverage",
    "test:types": "pest --type-coverage",
    "analyse": "php -d memory_limit=-1 ./vendor/bin/phpstan analyse",
    "check": [
      "@test",
      "@analyse"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "require-dev": {
    "laravel/pint": "^1.22",
    "pestphp/pest": "^3.0",
    "pestphp/pest-plugin-laravel": "^3.0",
    "pestphp/pest-plugin-arch": "^3.0",
    "pestphp/pest-plugin-type-coverage": "^3.0",
    "orchestra/testbench": "^9.0|^10.0",
    "phpstan/phpstan": "^1.10",
    "larastan/larastan": "^2.0"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
